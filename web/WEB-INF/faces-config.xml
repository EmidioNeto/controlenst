<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    
    <converter>
        <converter-id>cursoConverter</converter-id>  
        <converter-class>nst.controlenst.converter.CursoConverter</converter-class>  
    </converter>
    
    <converter>
        <converter-id>motivoSaidaConverter</converter-id>  
        <converter-class>nst.controlenst.converter.MotivoSaidaConverter</converter-class>  
    </converter>
    
    <converter>
        <converter-id>projetoConverter</converter-id>  
        <converter-class>nst.controlenst.converter.ProjetoConverter</converter-class>  
    </converter>
    
    <converter>
        <converter-id>situacaoConverter</converter-id>  
        <converter-class>nst.controlenst.converter.SituacaoConverter</converter-class>  
    </converter>
    
    <converter>
        <converter-id>tipoIngressoConverter</converter-id>  
        <converter-class>nst.controlenst.converter.TipoIngressoConverter</converter-class>  
    </converter>
    
    <converter>
        <converter-id>vinculoConverter</converter-id>  
        <converter-class>nst.controlenst.converter.VinculoConverter</converter-class>  
    </converter>
    
    
    
    
    
    
    
    
    
    
    
    <managed-bean>
        <managed-bean-name>ParticipanteBean</managed-bean-name>
        <managed-bean-class>nst.controlenst.view.bean.ParticipanteBEAN</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>    
    
    <managed-bean>
        <managed-bean-name>ProjetoBean</managed-bean-name>
        <managed-bean-class>nst.controlenst.view.bean.ProjetoBEAN</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>logado</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_projetos</from-outcome>
            <to-view-id>/restrito/listProjetos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>list_paticipantes</from-outcome>
            <to-view-id>/restrito/listParticipantes.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>novo_participante</from-outcome>
            <to-view-id>/restrito/newParticipante.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>newar_participante</from-outcome>
            <to-view-id>/restrito/newParticipante.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>tela_relatorios</from-outcome>
            <to-view-id>/restrito/relatorios.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>novo_projeto</from-outcome>
            <to-view-id>/restrito/newProjeto.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/restrito/newParticipante.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cadastroSucesso</from-outcome>
            <to-view-id>/restrito/listParticipantes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cadastroErro</from-outcome>
            <to-view-id>/restrito/newParticipante.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/restrito/editParticipante.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cadastroSucesso</from-outcome>
            <to-view-id>/restrito/listParticipantes.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cadastroErro</from-outcome>
            <to-view-id>/restrito/editParticipante.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/restrito/listParticipantes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editar_participante</from-outcome>
            <to-view-id>/restrito/editParticipante.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
